pragma solidity ^0.4.24;

contract If 
{
    function exampleIf() public view returns(uint)
    {
        uint n=50;
        
        if(n>25)
        {
            n++;
        }
        else if(n==25)
        {
            n+=5;
        }
        else
        {
            n--;
        }
        return n;
    }
}
